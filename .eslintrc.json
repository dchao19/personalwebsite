{
    "extends": ["google", "plugin:react/recommended"],
    "installedESLint": true,
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "max-len": 0,
        "no-else-return": 0
    },
    "globals": {
        "document": true,
        "window": true
    }
}